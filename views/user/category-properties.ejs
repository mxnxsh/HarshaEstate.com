<%- include("./../user/button.ejs")%>

<section class="ftco-section">
	<div class="container">
		<div class="row">
			<% products.forEach(function(p){ %>
			<div class="col-md-4 <%=p.isOn==='unavailable'? 'd-none' : '' %>">
				<div class="property-wrap ftco-animate">
					<a href="properties/<%=p.category%>/<%=p.slug%>/<%=p._id%>"
						class="img"
						style="background-image: url('/uploads/avatar/<%=p.avatar%>');"></a>
					<div class="text">
						<p class="price">
							<span class="orig-price">â‚¹<%=p.price%></span>
						</p>
						<ul class="property_list">
							<li><span class="flaticon-bed"></span><%=p.category%></li>
							<li><span class="flaticon-bathtub"></span><%=p.bathroom%></li>
							<li>
								<span class="flaticon-floor-plan"></span><%=p.floorArea%> sqft
							</li>
						</ul>
						<h3><a href="#"><%=p.title%></a></h3>
						<span class="location"><%=p.location%></span>
						<a href="#"
							class="d-flex align-items-center justify-content-center btn-custom">
							<span class="ion-ios-heart"></span>
						</a>
					</div>
				</div>
			</div>
			<% }); %>
		</div>
